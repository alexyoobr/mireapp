  object qryLista: TFDQuery
    ActiveStoredUsage = []
    Connection = UniMainModule.conlocal
    FetchOptions.AssignedValues = [evRowsetSize]
    FetchOptions.RowsetSize = 1000
    SQL.Strings = (
      
        'select e.id as SkuId, e.referencia as ProductId, e.idcor as Colo' +
        'r, e.idtamanho as Size, e.saldo-e.pedido as Quantity, p.idmarca ' +
        'as Brand, p.descricao as Name, p.preco1 as Price, p.ncm, p.idori' +
        'gem as origem, p.pesol/1000 as peso, p.preco4 as saleprice, p.ti' +
        'tuloseo as Title, p.descricaoseo as Description, p.composicao as' +
        ' Composition, i.url1 as ImageUrl1, i.url2 as ImageUrl2, i.url3 a' +
        's ImageUrl3, i.url4 as ImageUrl4 from `0015`.estoque e left join' +
        ' `0015`.produto p on e.referencia=p.id left join `0015`.imagem i' +
        ' on e.referencia=i.id and e.idcor=i.cor where p.vesti and e.id<>' +
        'e.referencia and e.saldo-e.pedido>=0 and p.preco1>0 order by ult' +
        'imaalteracao desc')
