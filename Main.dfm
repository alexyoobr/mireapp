    UpdateOptions.CheckReadOnly = False
    SQL.Strings = (
      
        'select i.referencia as id, e.saldo, "" as idcor,"" as  idtamanho' +
        ', sum(if(i.oper="V", i.quant, 0)) '
      'as quant, sum(if(i.oper="D", i.quant, 0)) as dev, '
      
        'i.referencia, p.descricao, p.idmarca, p.idcolecao, p.idcategoria' +
        ', p.idmodelo, '
      
        'p.datacadastro, p.lancamento, sum(i.total-i.desconto)/sum(i.quan' +
        't) as precomedio, '
      
        'p.precoinicial from `0132`.item i left join `0132`.produto p on ' +
        'i.referencia=p.id and p.tipo="P" left join (select referencia, s' +
        'um(saldo) as saldo, '
      
        'tipo from `0132`.estoque group by referencia) e on i.referencia=' +
        'e.referencia and e.tipo="P" left join `0132`.venda v on i.idvend' +
        'a=v.idvenda where v.status>"E" '
      
        'and v.data between '#39'2022-10-01'#39' and '#39'2023-01-09'#39' and v.idloja=3 ' +
        'group by i.referencia')
